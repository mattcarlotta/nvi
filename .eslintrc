{
    "parser": "@typescript-eslint/parser",
    "plugins": ["@typescript-eslint"],
    "extends": [
        "eslint:recommended",
        "plugin:prettier/recommended",
        "plugin:@typescript-eslint/recommended"
    ],
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "no-continue": 0,
        "@typescript-eslint/explicit-function-return-type": 0,
        "no-console": 0,
        "camelcase": 0,
        "consistent-return": 0,
        "no-underscore-dangle": 0
    }
}
